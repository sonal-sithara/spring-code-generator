{
  "name": "spring-code-generator",
  "displayName": "Spring Code Generator",
  "description": "Spring Code Generator",
  "icon": "image/logo.png",
  "publisher": "SonalSithara",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:spring-code-generator.createController"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "spring-code-generator.createController",
        "title": "Create Controller"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "Spring Code Generator"
        }
      ],
      "Spring Code Generator": [
        {
          "command": "spring-code-generator.createController"
        }
      ]
    },
    "submenus": [
      {
        "label": "Spring Code Generator",
        "id": "Spring Code Generator"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "fs": "^0.0.1-security"
  }
}
