{
  "name": "spring-code-generator",
  "displayName": "Spring Code Generator",
  "description": "A extension for Visual Studio Code based on java spring framework.This extension can generate necessary file in spring (Controller, Service, Entity, Dto) or generate crud operations with controller or just crud operations in your spring application.",
  "icon": "image/logo.png",
  "publisher": "SonalSithara",
  "version": "1.2.1",
  "repository": "https://github.com/sonal-sithara/spring-code-generator",
  "license": "MIT",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:spring-code-generator.createController",
    "onCommand:spring-code-generator.createControllerWithCrud",
    "onCommand:spring-code-generator.createEntity",
    "onCommand:spring-code-generator.createDto",
    "onCommand:spring-code-generator.createRepository",
    "onCommand:spring-code-generator.createEntityWithLombok",
    "onCommand:spring-code-generator.createDtoWithLombok",
    "onCommand:spring-code-generator.createException",
    "onCommand:spring-code-generator.createGlobalExceptionHandler",
    "onCommand:spring-code-generator.createApplicationProperties",
    "onCommand:spring-code-generator.createApplicationYml",
    "onCommand:spring-code-generator.createMapper",
    "onCommand:spring-code-generator.createConverter",
    "onCommand:spring-code-generator.createSecurityConfig",
    "onCommand:spring-code-generator.createTestClass",
    "onCommand:spring-code-generator.createServiceImpl",
    "onCommand:spring-code-generator.createRequestDto",
    "onCommand:spring-code-generator.createResponseDto",
    "onCommand:spring-code-generator.createBatchModule"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "spring-code-generator.createController",
        "title": "Create Controller",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createControllerWithCrud",
        "title": "Create Controller With Crud",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createEntity",
        "title": "Create Entity",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createEntityWithLombok",
        "title": "Create Entity With Lombok",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createDto",
        "title": "Create Dto",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createDtoWithLombok",
        "title": "Create Dto With Lombok",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createRepository",
        "title": "Create Repository",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createException",
        "title": "Create Exception",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createGlobalExceptionHandler",
        "title": "Create Global Exception Handler",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createApplicationProperties",
        "title": "Create Application Properties",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createApplicationYml",
        "title": "Create Application YML",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createMapper",
        "title": "Create Mapper",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createConverter",
        "title": "Create Converter",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createSecurityConfig",
        "title": "Create Security Configuration",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createTestClass",
        "title": "Create Test Class",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createServiceImpl",
        "title": "Create Service Implementation",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createRequestDto",
        "title": "Create Request DTO",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createResponseDto",
        "title": "Create Response DTO",
        "category": "Spring Code Generator"
      },
      {
        "command": "spring-code-generator.createBatchModule",
        "title": "Create Batch Module (Complete Module)",
        "category": "Spring Code Generator"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "Spring Code Generator"
        }
      ],
      "Spring Code Generator": [
        {
          "command": "spring-code-generator.createController",
          "group": "controller"
        },
        {
          "command": "spring-code-generator.createControllerWithCrud",
          "group": "controller"
        },
        {
          "command": "spring-code-generator.createService",
          "group": "service"
        },
        {
          "command": "spring-code-generator.createDto",
          "group": "dto"
        },
        {
          "command": "spring-code-generator.createRepository",
          "group": "repository"
        },
        {
          "command": "spring-code-generator.createException",
          "group": "exception"
        },
        {
          "command": "spring-code-generator.createGlobalExceptionHandler",
          "group": "exception"
        },
        {
          "command": "spring-code-generator.createApplicationProperties",
          "group": "config"
        },
        {
          "command": "spring-code-generator.createApplicationYml",
          "group": "config"
        },
        {
          "command": "spring-code-generator.createMapper",
          "group": "mapper"
        },
        {
          "command": "spring-code-generator.createConverter",
          "group": "mapper"
        },
        {
          "command": "spring-code-generator.createSecurityConfig",
          "group": "security"
        },
        {
          "command": "spring-code-generator.createTestClass",
          "group": "test"
        },
        {
          "command": "spring-code-generator.createServiceImpl",
          "group": "service"
        },
        {
          "command": "spring-code-generator.createRequestDto",
          "group": "dto"
        },
        {
          "command": "spring-code-generator.createResponseDto",
          "group": "dto"
        },
        {
          "command": "spring-code-generator.createDtoWithLombok",
          "group": "dto"
        },
        {
          "command": "spring-code-generator.createEntityWithLombok",
          "group": "entity"
        },
        {
          "command": "spring-code-generator.createBatchModule",
          "group": "batch"
        }
      ]
    },
    "submenus": [
      {
        "label": "Spring Code Generator",
        "id": "Spring Code Generator"
      }
    ],
    "snippets": [
      {
        "language": "java",
        "path": "./out/snippets/snippets.code-snippets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "fs": "^0.0.2"
  }
}
